    <!--contact map start-->
    <div class="contact_map mt-100">
        <div class="map-area">
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3917.385995960225!2d106.79714861480217!3d10.934187192214607!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x0!2zMTDCsDU2JzAzLjEiTiAxMDbCsDQ3JzU3LjYiRQ!5e0!3m2!1svi!2s!4v1633103052465!5m2!1svi!2s" width="100%" height="460px" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
        </div>
    </div>
    <!--contact map end-->

    <!--contact area start-->
    <div class="contact_area">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-12">
                    <div class="contact_message content">
                        <h3>contact us</h3>
                        <p>Please feel free to contact us if you want to cooperate.</p>
                        <ul>
                            <li><i class="fa fa-fax"></i> Address: Bien Hoa City, Dong Nai Province, Viet Nam.</li>
                            <li><i class="fa fa-envelope-o"></i> <a href="mailto: hppots.vn@gmail.com">hppots.vn@gmail.com</a></li>
                            <li><i class="fa fa-phone"></i><a href="tel:+84966900010">+84966900010</a> </li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-6 col-md-12">
                    <div class="contact_message form">
                        <h3>Tell us your project</h3>
                        <form id="contact-form" method="post" action="/add-contact">
                            <p>
                                <label> Your Name (required)</label>
                                <input name="name" placeholder="Name *" type="text">
                            </p>
                            <p>
                                <label> Your Email (required)</label>
                                <input name="email" placeholder="Email *" type="email">
                            </p>
                            <p>
                                <label> Your Country (required)</label>
                                <input name="country" placeholder="Country *" type="text">
                            </p>
                            <div class="contact_textarea">
                                <label> Your Message</label>
                                <textarea placeholder="Message *" name="message" class="form-control2"></textarea>
                            </div>
                            <button type="submit"> Send</button>
                            <p class="form-messege"></p>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--contact area end-->

    <!--brand area start-->
    <!--brand area end-->

